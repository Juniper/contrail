{
    "Enable": [
        "deadcode",
        "errcheck",
        "goconst",
        "gocyclo",
        "gofmt",
        "goimports",
        "golint",
        "gosec",
        "gosimple",
        "gotype",
        "gotypex",
        "ineffassign",
        "interfacer",
        "lll",
        "maligned",
        "misspell",
        "nakedret",
        "structcheck",
        "unconvert",
        "unparam",
        "unused",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "Exclude": [
        "gen_.*.go",
        "generated.pb.go",
        "Subprocess launching with variable.*(gosec)",
        "TLS InsecureSkipVerify.*(gosec)",
        "parameter .* always receives.*(unparam)"
    ],
    "Skip": [
        "pkg/services/mock",
        "pkg/types/mock",
        "pkg/types/ipam/mock"
    ],
    "Test": true,
    "Vendor": true,
    "Aggregate": true,
    "Sort": [
        "path",
        "line"
    ],
    "Deadline": "5m",
    "Concurrency": 2,
    "LineLength": 120
}
