{
    "Enable": [
        "deadcode",
        "dupl",
        "errcheck",
        "gas",
        "goconst",
        "gocyclo",
        "gofmt",
        "goimports",
        "golint",
        "gosimple",
        "gotype",
        "gotypex",
        "ineffassign",
        "interfacer",
        "lll",
        "maligned",
        "misspell",
        "nakedret",
        "structcheck",
        "unconvert",
        "unparam",
        "unused",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "Exclude": [
        "Subprocess launching with variable.*(gas)",
        "TLS InsecureSkipVerify.*(gas)"
    ],
    "Skip": [
        "pkg/types/mock",
        "pkg/types/ipam/mock",
        "pkg/compilationif",
        "pkg/db",
        "pkg/models",
        "pkg/services"
    ],
    "Test": true,
    "Vendor": true,
    "Aggregate": true,
    "Sort": [
        "path"
    ],
    "Deadline": "10m",
    "Concurrency": 2,
    "LineLength": 120,
    "DuplThreshold": 115
}
