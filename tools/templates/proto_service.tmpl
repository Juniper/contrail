syntax = "proto3";
package github.com.Juniper.contrail.pkg.services;

import "github.com/gogo/protobuf/gogoproto/gogo.proto";
import "github.com/Juniper/contrail/pkg/models/generated.proto";

option (gogoproto.protosizer_all) = true;
option (gogoproto.sizer_all) = false;
option go_package = "services";

service ContrailService {
{% for schema in schemas %}{% if schema.Type != "abstract" and schema.ID %}
rpc Get{{ schema.JSONSchema.GoName }} (github.com.Juniper.contrail.pkg.models.ObjectID) returns (github.com.Juniper.contrail.pkg.models.Resource{{ schema.JSONSchema.GoName }});
rpc List{{ schema.JSONSchema.GoName }} (github.com.Juniper.contrail.pkg.models.ListSpec) returns (github.com.Juniper.contrail.pkg.models.ListResources{{ schema.JSONSchema.GoName }});
rpc Create{{ schema.JSONSchema.GoName }} (github.com.Juniper.contrail.pkg.models.Resource{{ schema.JSONSchema.GoName }}) returns (github.com.Juniper.contrail.pkg.models.Resource{{ schema.JSONSchema.GoName }});
rpc Update{{ schema.JSONSchema.GoName }} (github.com.Juniper.contrail.pkg.models.ResourceUpdate{{ schema.JSONSchema.GoName }}) returns (github.com.Juniper.contrail.pkg.models.Resource{{ schema.JSONSchema.GoName }});
rpc Delete{{ schema.JSONSchema.GoName }} (github.com.Juniper.contrail.pkg.models.ObjectID) returns (github.com.Juniper.contrail.pkg.models.ObjectID);
{% endif %}{% endfor %}
}
